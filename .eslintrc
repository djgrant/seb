{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [2],
    "no-multi-spaces": [0],
    "nonew": false
  },
  "globals": {
        "_": true
    }
}
